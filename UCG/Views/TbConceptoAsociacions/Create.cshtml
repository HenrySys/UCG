@using UCG.Models.ViewModels;
@using static UCG.Models.TbConceptoAsociacion;
@model ConceptoAsociacionViewModel

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>TbConceptoAsociacion</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="IdAsociacion" class="control-label"></label>
                <select asp-for="IdAsociacion" class ="form-select" asp-items="ViewBag.IdAsociacion">
                    <option disabled="" selected="">Seleccione una asociacion</option>
                </select>
            </div>
            <div class="form-group">
                <label asp-for="IdConcepto" class="control-label"></label>
                <select asp-for="IdConcepto" class ="form-select" asp-items="ViewBag.IdConcepto">
                    <option disabled="" selected="">Seleccione un concepto</option>
                </select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
