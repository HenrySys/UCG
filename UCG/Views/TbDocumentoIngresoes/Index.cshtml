            <td>
                @Html.DisplayFor(modelItem => item.IdMovimientoIngresoNavigation.IdMovimientoIngreso)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdDocumentoIngreso">Edit</a> |
